{
  "name": "NekaBlock",
  "version": "2.5",
  "description": "a simple ad blocker",
  "homepage": "https://sourceforge.net/projects/nekablockaab",
  "permissions": ["webRequest", "webRequestBlocking", "http://*/", "https://*/"],
  "background": {
	"scripts": ["blocked_domains.js", "background.js"]
  },
  
"browser_action": {
    "default_popup": "popup.html"
  },
"manifest_version": 2
}